{
  "name": "n8n-nodes-expensify",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Expensify providing 8 resources and 25+ operations for expense management, report automation, policy administration, and financial reconciliation.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-expensify.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-expensify#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-expensify/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "expensify",
    "expense-management",
    "expense-reports",
    "receipt-scanning",
    "reimbursement",
    "accounting",
    "finance",
    "automation"
  ],
  "main": "dist/nodes/Expensify/Expensify.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExpensifyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Expensify/Expensify.node.js",
      "dist/nodes/Expensify/ExpensifyTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"nodes/**/*.svg\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
